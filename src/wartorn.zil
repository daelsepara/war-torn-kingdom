		<PUTP .STORY ,P?DOOM T>
		<TELL .MESSAGE>
		<SETG CONTINUE-TO-CHOICES F>
	)(ELSE
		<PUTP .STORY ,P?DOOM F>
		<TELL "You lost " N .DAMAGE " stamina">
	)>
	<HLIGHT 0>
