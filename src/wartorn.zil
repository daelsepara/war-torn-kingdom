<ROUTINE PRINT-CODEWORDS (CODEWORDS)
	<PRINT-LIST .CODEWORDS "and ">>

<ROUTINE PRINT-LIST (ITEMS "OPT" LASTFIX "AUX" COUNT)
	<COND (.ITEMS
		<SET COUNT <GET .ITEMS 0>>
		<COND (<G? .COUNT 0>
			<TELL " (">
			<DO (I 1 .COUNT)
				<COND (<G? .I 1>
					<COND (<NOT <EQUAL? .COUNT 2>>
						<TELL ", ">
					)(ELSE
						<TELL " ">
					)>
				)>
				<COND (<AND <G? .COUNT 1> <EQUAL? .I .COUNT> .LASTFIX> <TELL .LASTFIX>)>
				<HLIGHT ,H-ITALIC>
				<TELL D <GET .ITEMS .I>>
				<HLIGHT 0>
			>
			<TELL ")">
		)>
