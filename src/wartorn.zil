	<TELL " " N .COST " " D ,CURRENCY ,PERIOD-CR>
	<HLIGHT 0>
	<SETG MONEY <- ,MONEY .COST>>
	<COND (<L? ,MONEY 0> <SETG MONEY 0>)>
	<UPDATE-STATUS-LINE>>

