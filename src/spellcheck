#!/bin/bash
spell -n wartorn.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|READBUF|LEXBUF|GETB|CONTBIT|OPENBIT|VOWELBIT|NARTICLEBIT|NDESCBIT|INTBL|PLURALBIT|PERSONBIT|TOUCHBIT|TAKEBIT|COLOR|CURSET|LOWCORE|WEAPONBIT|PRINTI|SCRH|PRINTC|PROPDEF|RELEASEID|IFID|NPC|XZIP|PRINTN|GIVEBAG|PRICELIST|DEFENSE|LASTFIX|ZIL|ADECB|: SD$|: CT$|: OBJ$|: STR$|: KER$" | grep -i -v -Ev "minilib|gamebook|harbourmaster|unweathered|sorcerous|merfolk|knobbly|realize|golem|faeries|manlike|ratmen|shortswords|barque|ghoulbite|defense|initialize|lockpicks|fireheart|skysong|mage|backstreets|eldritch|pentacle|reroll|eyepatch|terrorizing|recognized|clifftop|vaporized|wishport|undead|quayside|recognizes|dodgy|octopoid|recognize|coldbleak|guildmaster|vasty|porticos|chested|forelimbs|manbeasts|realizing" | grep -i -v -Ev ": maka$|: marlock$|: marloes$|: ankon$|: konu$|: pyletes$|: elnir$|: tyrnai$|: tyrnai's$|: venefax$|: trefoille$|: harkun$|: harkuna$|: sokara$|: velis$|: alvir$|: valmir$|: uttaku$|: ebron$|: uttakin$|: corin$|: yellowport$|: sokaran$|: royzer$|: nergan$|: gobrash$|: vorkung$|: dweomer$|: marlock's$|: aklar$|: vayss$|: molhern$|: shadar$|: oannes$|: marana$|: andriel$|: chalor$|: sig$|: liana$|: astariel$|: separa$|: larun$|: almir$|: veneris$|: nagil$|: lauria$|: elnir's$|: baru$|: caran$|: bo'sun$|: sokarans$|: badogor$|: sul$|: umm$|: er$|: golnir$|: conflass$|: sokar$|: curstmoor$|: argh$|: ziusudra$|: pssst$|: pre$|: ker'ilk$|: negran$|: pootmar$|: bradok$|: tos$|: palanquin$|: gorlock$|: skabb$|: yanryt$|: brilon$|: thar$|: griffon$|: amcha$|: fourze$|: damor$|: nerech" |uniq
