#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|: CT$|OBJ$" | grep -i -v -Ev "harbormaster|unweathered|sorcerous|merfolk" | grep -i -v -Ev ": maka$|: marlock$|: marloes$|: ankon$|: konu$|: pyletes$|: elnir$|: tyrnai$|: venefax$|: trefoille$|: harkun$|: harkuna$|: sokara$|: velis$|: alvir$|: valmir$|: uttaku$|: ebron$|: uttakin$|: corin$|: yellowport$" |uniq
