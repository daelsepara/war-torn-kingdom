#!/bin/bash
spell -n wartorn.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|READBUF|LEXBUF|GETB|CONTBIT|OPENBIT|VOWELBIT|NARTICLEBIT|NDESCBIT|INTBL|PLURALBIT|PERSONBIT|TOUCHBIT|TAKEBIT|COLOR|CURSET|LOWCORE|WEAPONBIT|PRINTI|SCRH|PRINTC|PROPDEF|RELEASEID|IFID|NPC|XZIP|PRINTN|GIVEBAG|PRICELIST|DEFENSE|LASTFIX|ADECB|ZIL|: SD$|: CT$|OBJ$" | grep -i -v -Ev "minilib|gamebook|harbourmaster|unweathered|sorcerous|merfolk|knobbly|realize|golem|faeries|manlike|ratmen|shortswords|barque|ghoulbite|defense|initialize|lockpicks|fireheart|skysong|mage|backstreets|eldritch|pentacle|reroll|eyepatch|terrorizing|recognized|clifftop|vaporized|wishport|undead|quayside|recognizes" | grep -i -v -Ev ": maka$|: marlock$|: marloes$|: ankon$|: konu$|: pyletes$|: elnir$|: tyrnai$|: venefax$|: trefoille$|: harkun$|: harkuna$|: sokara$|: velis$|: alvir$|: valmir$|: uttaku$|: ebron$|: uttakin$|: corin$|: yellowport$|: sokaran$|: royzer$|: nergan$|: gobrash$|: vorkung$|: dweomer$|: marlock's$|: aklar$|: vayss$|: molhern$|: shadar$|: oannes$|: marana$|: andriel$|: chalor$|: sig$|: liana$|: astariel$|: separa$|: larun$|: almir$|: veneris$|: nagil$|: lauria$|: elnir's$|: baru$|: caran$|: bo'sun$|: sokarans$|: badogor$|: sul$|: umm$|: er$|: golnir$|: conflass$|: sokar$|: curstmoor$" |uniq
